<ul>
    {{# each userList}}
        <li class="container">
            <div>
                <ul>
                    {{# each items}}
                        <li><p class="message">"{{name}}"</p></li>
                    {{/ each}}
                </ul>
            </div>
            <br>
            <div>&mdash;{{first_name}} {{last_name}} ({{email}})</div>
            <div><a href="/users/{{_id}}">View User</a> <a class="delete" href="/users/delete/{{_id}}" role="button">Delete User</a></div>
        </li>
        <br>
    {{/ each}}
</ul>
